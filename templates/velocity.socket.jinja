[Unit]
Description={{ minecraft_service_name | title }} Server Console

[Socket]
ListenFIFO={{ minecraft_console_fifo }}
SocketUser={{ minecraft_user }}
SocketGroup={{ minecraft_group }}
SocketMode={{ minecraft_console_fifo_mode }}
RemoveOnStop=true

[Install]
WantedBy=sockets.target
